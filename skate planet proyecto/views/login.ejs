<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head') %>
</head>

<body>
    <%- include('./partials/header') %>
    
<br>

        <span class="span-ing-prod">
            <div class="ingresar-productos">
              
                <form class="form-tabla" action="/users/login" method="POST" class="form-login">
                
                    <h2 class="titulo-registro">Ingresá!</h2>
                    <!-- nombre, codigo, precio, material, categoría, medida, origen, descripción, -->
                    <br>
                    
                    <br>
                    <label for="email"></label>
                    <input type="text" name="email" id="email" placeholder="email" >
                    <div class="error">
                        <% if(locals.errors && errors.email){ %>
                          <%= errors.email.msg %>
                        <% } %>
                      </div>
                    <br>
                    <label for="password"></label>
                    <input type="text" name="password" id="password" placeholder="*******" >
                    <div class="error">
                        <% if(locals.errors && errors.password){ %>
                          <%= errors.password.msg %>
                        <% } %>
                      </div>
                    <br>
                   
                    <input type="submit" value="Ingresar">
                    <!-- <img src="images/esqueleto-bailando-esqueleto.gif" alt="gif" class="gif-esqueleto"> -->
                </form>
            </div>
        </span>
        <br>
        <%- include('./partials/footer') %>
</body>

</html>